#!/usr/bin/env node
var program = require('commander')
var pkg = require('../package.json')

program
  .version(pkg.version)
  .command('dev', 'Run a development server')
  .command('build', 'Build the production bundles')
  .command('serve', 'Run a production server')
  .command('init [type]', 'Initialize a new React project in the current directory')
  .command('register', 'Output options for babel-register as JSON')

program.parse(process.argv)
